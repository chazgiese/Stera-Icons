{
  "name": "stera-icons",
  "version": "5.0.1",
  "type": "module",
  "description": "A lightweight, tree-shakeable React icon library",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup && pnpm build:metadata",
    "build:dev": "tsup --config tsup.config.dev.ts && pnpm build:metadata",
    "build:metadata": "node ../../scripts/build-icons.js ../../icons-export.json",
    "dev": "tsup --watch --config tsup.config.dev.ts",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.1.0",
    "@types/react": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "jsdom": "^23.0.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0"
  },
  "keywords": [
    "react",
    "icons",
    "svg",
    "typescript",
    "tree-shakeable"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chazgiese/Stera-Icons.git"
  },
  "license": "MIT"
}
